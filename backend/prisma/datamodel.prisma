type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  updatedAt: DateTime!
  createdAt: DateTime!
}

type Lockbox {
  id: ID! @unique
  amount: Float!
  branch: String!
  box: String!
  time: String!
  date: String!
  transactions: [LockboxTransaction!]!
    @relation(name: "TransactionToLockbox", onDelete: CASCADE)
  updatedAt: DateTime!
  createdAt: DateTime!
}

type LockboxTransaction {
  id: ID! @unique
  amount: Float!
  lockbox: Lockbox! @relation(name: "TransactionToLockbox", onDelete: SET_NULL)
}
